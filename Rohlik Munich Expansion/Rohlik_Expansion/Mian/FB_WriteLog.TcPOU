<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.12">
  <POU Name="FB_WriteLog" Id="{22896bc6-5e49-4c45-8372-5786ffe05552}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_WriteLog
VAR_IN_OUT
	bWrite:BOOL;
END_VAR
VAR_INPUT	
	sFileName:STRING;
	sMessage : STRING;
END_VAR

VAR_OUTPUT
	OK:BOOL;
	Busy:BOOL;
	ErrorMsg:STRING(255);
	bError:BOOL;
	ErrorID:UDINT;
END_VAR

VAR
	fileOpen:FB_FileOpen;
	fileWrite:FB_FileWrite;
	fileClose:FB_FileClose;
	fileDelete:FB_FileDelete;
	hFile:UINT;
	stringBuffer:STRING(255);
	eWriteStep:INT;
	sFileExtendName:STRING(255);
	LengthOfFileName:INT;
	
	iArrNo:UINT;
	rtWriteCSV:R_trig;
	bReset: BOOL;
	GetTimeStartDelay: Ton;
	NT_GetTime1: NT_GetTime;
	TIMESTR1: TIMESTRUCT;
	sCurrentTime_Temp: STRING;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[NT();

rtWriteCSV(CLK:=bWrite,Q=>);

CASE eWriteStep OF
0:
	Busy:=FALSE;
	IF bWrite   THEN
		bWrite := FALSE;
		OK:=FALSE;
		Busy:=TRUE;
		iArrNo:=0;
		bError:=FALSE;
		ErrorMsg:='';
		ErrorID:=0;
		eWriteStep:=100;	
	END_IF
	
(*判断文件扩展名是否正确*)	
100:	
	fileOpen(bExecute:=TRUE);
	IF fileOpen.bBusy THEN
		eWriteStep:=210;	
	END_IF

210:
	fileOpen(bExecute:=FALSE);
	IF NOT fileOpen.bBusy AND NOT fileOpen.bError THEN
		eWriteStep:=300;	
	END_IF
	IF fileOpen.bError THEN
		ErrorMsg:='FileOpenError';
		ErrorID:=fileOpen.nErrId;
		eWriteStep:=9999;	
	END_IF

300:
		stringBuffer:='';
		stringBuffer:=CONCAT(stringBuffer,sCurrentTime_Temp);
		stringBuffer:=CONCAT(stringBuffer,'->');
		stringBuffer:=CONCAT(stringBuffer,sMessage);
		stringBuffer:=CONCAT(stringBuffer,'$n');
		fileWrite(bExecute:=TRUE,cbWriteLen:=LEN(stringBuffer));
		IF fileWrite.bBusy THEN
			eWriteStep:=310;	
		END_IF
310:
	fileWrite(bExecute:=FALSE);
	IF NOT fileWrite.bBusy AND NOT fileWrite.bError THEN
		eWriteStep:=500;	
	END_IF
	IF fileWrite.bError THEN
		ErrorMsg:='FileWriteError';
		ErrorID:=fileWrite.nErrId;
		eWriteStep:=9999;	
	END_IF

500:
	fileClose(bExecute:=TRUE);
	IF fileClose.bBusy THEN
		eWriteStep:=510;	
	END_IF

510:
	fileClose(bExecute:=FALSE);
	IF NOT fileClose.bBusy AND NOT fileClose.bError THEN
		eWriteStep:=600;	
	END_IF
	IF fileClose.bError THEN
		ErrorMsg:='FileOpenError';
		ErrorID:=fileOpen.nErrId;
		eWriteStep:=9999;	
	END_IF
	
600:
	OK:=TRUE;
	eWriteStep:=0;	
		
9999:
	bError:=TRUE;
	Busy:=FALSE;
	IF bReset THEN
		OK:=FALSE;
		ErrorID:=0;
		eWriteStep:=0;	
	END_IF
	
END_CASE

Act01_CallFB();]]></ST>
    </Implementation>
