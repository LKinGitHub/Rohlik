    <Action Name="NT" Id="{e6e039c3-b261-4141-9510-d89e6dc74d24}">
      <Implementation>
        <ST><![CDATA[
GetTimeStartDelay(IN:=NOT GetTimeStartDelay.Q  , PT:=T#1S , Q=> , ET=> );
NT_GetTime1(
	NETID:= ,
	START:=GetTimeStartDelay.Q ,
	TMOUT:= , 
	BUSY=> ,
	ERR=> ,
	ERRID=> ,
	TIMESTR=>TIMESTR1 );

TIMESTR1.wYear;		(*年*)
TIMESTR1.wMonth;		(*月*)
TIMESTR1.wDay;		(*日*)
TIMESTR1.wHour;		(*时*)
TIMESTR1.wMinute;		(*分*)
TIMESTR1.wSecond;		(*秒*)
TIMESTR1.wMilliseconds;	(*毫秒*)
TIMESTR1.wDayOfWeek;	(*星期*)

sCurrentTime_Temp := '';
sCurrentTime_Temp := CONCAT(sCurrentTime_Temp,WORD_TO_STRING(TIMESTR1.wYear));
 sCurrentTime_Temp:= CONCAT(sCurrentTime_Temp,'-');
 sCurrentTime_Temp:= CONCAT(sCurrentTime_Temp,WORD_TO_STRING(TIMESTR1.wMonth));
 sCurrentTime_Temp:= CONCAT(sCurrentTime_Temp,'-');
 sCurrentTime_Temp:= CONCAT(sCurrentTime_Temp,WORD_TO_STRING(TIMESTR1.wDay));
 sCurrentTime_Temp:= CONCAT(sCurrentTime_Temp,' ');
 sCurrentTime_Temp:= CONCAT(sCurrentTime_Temp,WORD_TO_STRING(TIMESTR1.wHour));
sCurrentTime_Temp := CONCAT(sCurrentTime_Temp,':');
 sCurrentTime_Temp:= CONCAT(sCurrentTime_Temp,WORD_TO_STRING(TIMESTR1.wMinute));
 sCurrentTime_Temp:= CONCAT(sCurrentTime_Temp,':');
 sCurrentTime_Temp:= CONCAT(sCurrentTime_Temp,WORD_TO_STRING(TIMESTR1.wSecond));
]]></ST>
